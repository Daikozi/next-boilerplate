{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint \"{components,pages,redux,utils,hooks}/**/*.{ts,tsx}\"",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:icones:js:fix": "eslint \"components/designSystem/icons/**/*.{ts,tsx}\" --report-unused-disable-directives --fix",
    "check-types": "tsc --project tsconfig.json",
    "prettier": "prettier .",
    "check-format": "npm run prettier -- --check",
    "validate": "npm-run-all --parallel lint check-types check-format",
    "format": "npm run prettier -- --write",
    "test": "jest",
    "test:group": "cross-env TZ=UTC jest --config=./jest.config.group.js",
    "test:ci": "npm run test:group -- --maxWorkers=25%",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:watch": "npm run test -- --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.3",
    "@next/font": "13.1.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "eslint-config-next": "13.1.1",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.41.5",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.5",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/eslint-plugin": "^11.10.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^8.3.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^5.75.0"
  }
}
